<Window x:Class="PV168_Project.windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PV168_Project"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Grid>
        <Button x:Name="ContinueButton" Content="Continue" HorizontalAlignment="Center" Margin="0,270,0,0" VerticalAlignment="Top" Cursor="Arrow" Click="Continue_Click"/>
        <TextBlock x:Name="GreetingText" HorizontalAlignment="Center" Margin="0,138,0,0" Text="Welcome to the tournament manager" TextWrapping="Wrap" VerticalAlignment="Top" FontFamily="Arial Black" FontSize="22"/>
    </Grid>
</Window>
